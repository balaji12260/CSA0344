#include <stdio.h>

int main() {
    int numbers[100], n;

    // Input how many numbers
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    // Input numbers
    printf("Enter %d numbers:\n", n);
    for (int i = 0; i < n; ++i) {
        scanf("%d", &numbers[i]);
    }

    // Check each number
    printf("Odd or Even Results:\n");
    for (int i = 0; i < n; ++i) {
        if (numbers[i] % 2 == 0)
            printf("%d is Even\n", numbers[i]);
        else
            printf("%d is Odd\n", numbers[i]);
    }

    return 0;
}
